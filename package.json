{
  "name": "safetnet-monorepo",
  "version": "1.0.0",
  "description": "Safe T Net - Security Management System",
  "scripts": {
    "postinstall": "node setup.js",
    "setup": "node setup.js",
    "setup:frontend": "cd safe-fleet-admin && npm install",
    "setup:backend": "node setup-backend.js",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node start-backend.js",
    "start:frontend": "cd safe-fleet-admin && npm run dev",
    "dev": "npm run start",
    "clean": "rimraf SafeTNet/venv SafeTNet/__pycache__ safe-fleet-admin/node_modules safe-fleet-admin/.next"
  },
  "keywords": [
    "security",
    "management",
    "admin"
  ],
  "author": "mayur533",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.10"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/geolocation": "^3.1.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native/new-app-screen": "0.82.1",
    "react-native-maps": "^1.15.9",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "date-fns": "^4.1.0",
    "react-native-sms-android": "^1.0.6",
    "nativewind": "^4.2.1",
    "react-native-volume-controller": "^1.3.3"
  }
}
